@model IEnumerable<UserViewModel>

<div class="text-center">
    <h1>Users</h1>
    <table class="table">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Birthday</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.Birthday.ToShortDateString()</td>
                </tr>
            }
        </tbody>
    </table>
</div>
